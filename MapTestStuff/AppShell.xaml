<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MapTestStuff.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MapTestStuff"
    xmlns:views="clr-namespace:MapTestStuff.Views">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent Title="Map Test"
                  ContentTemplate="{DataTemplate views:MapTest}"
                  Route="MapTest"/>

</Shell>
